<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kylemiller.cardgame.EndGameActivity">


    <TextView
        android:id="@+id/computer_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/score_Computer" />

    <TextView
        android:id="@+id/winning_name"
        android:layout_width="69dp"
        android:layout_height="22dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="158dp" />

    <TextView
        android:id="@+id/congrats"
        android:layout_width="119dp"
        android:layout_height="44dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="133dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/winning_name" />

    <TextView
        android:id="@+id/scoreHuman"
        android:layout_width="107dp"
        android:layout_height="29dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="139dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/congrats" />

    <TextView
        android:id="@+id/score_Computer"
        android:layout_width="107dp"
        android:layout_height="29dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="139dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/human_output_score" />

    <TextView
        android:id="@+id/human_output_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/scoreHuman" />

    <Button
        android:id="@+id/home_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/return_home"
        tools:layout_editor_absoluteX="147dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/computer_score" />

    <Button
        android:id="@+id/play_again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Button"
        app:layout_constraintTop_toBottomOf="@+id/home_button"
        tools:layout_editor_absoluteX="147dp" />
</android.support.constraint.ConstraintLayout>
